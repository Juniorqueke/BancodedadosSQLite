<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/layout_cadastro"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#0DF703"
    android:orientation="vertical">

    <ImageView
        android:id="@+id/img_logo_consulta"
        android:layout_width="match_parent"
        android:layout_height="139dp"
        android:background="#2FEC04"
        app:srcCompat="@drawable/baseline_mood_black_18dp" />

    <TextView
        android:id="@+id/txt_mensagem_consulta"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Consulta de dados"
        android:textAppearance="@style/TextAppearance.AppCompat.Display1"
        android:textColor="#FFFF00" />

    <TextView
        android:id="@+id/edt_nome_consulta"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Nome"
        android:textAppearance="@style/TextAppearance.AppCompat.Medium"
        android:textColor="#FFFF00" />

    <EditText
        android:id="@+id/edt_name_consulta"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="[Nome]"
        android:textAppearance="@style/TextAppearance.AppCompat.Medium"
        android:textColor="#FFFF00" />

    <TextView
        android:id="@+id/edt_email_consulta"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Email"
        android:textAppearance="@style/TextAppearance.AppCompat.Medium"
        android:textColor="#FFFF00" />

    <TextView
        android:id="@+id/edt_emailconsulta"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="[Email]"
        android:textAppearance="@style/TextAppearance.AppCompat.Medium"
        android:textColor="#FFFF00" />

    <EditText
        android:id="@+id/edt_telefone_consulta"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Telefone"
        android:textAppearance="@style/TextAppearance.AppCompat.Medium"
        android:textColor="#FFFF00" />

    <EditText
        android:id="@+id/edt_phone_consulta"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="[Telefone]"
        android:textAppearance="@style/TextAppearance.AppCompat.Medium"
        android:textColor="#FFFF00" />

    <LinearLayout
        android:id="@+id/layout_setas_consulta"
        android:layout_width="match_parent"
        android:layout_height="62dp"
        android:background="#1FF011"
        android:orientation="horizontal">

        <ImageView
            android:id="@+id/img_capAnterior_consulta"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:background="#FFFF00"
            app:srcCompat="@drawable/baseline_fast_rewind_black_18dp" />

        <ImageView
            android:id="@+id/img_voltar_consulta"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:background="#FFFF00"
            app:srcCompat="@drawable/baseline_skip_previous_black_18dp" />

        <ImageView
            android:id="@+id/img_play_consulta"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:background="#FFFF00"
            app:srcCompat="@drawable/baseline_play_arrow_black_18dp" />

        <ImageView
            android:id="@+id/img_avancar_consulta"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:background="#FFFF00"
            app:srcCompat="@drawable/baseline_skip_next_black_18dp" />

        <ImageView
            android:id="@+id/img_proximoCap_consulta"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:background="#FFFF00"
            app:srcCompat="@drawable/baseline_fast_forward_black_18dp" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:orientation="vertical">

        <TextView
            android:id="@+id/txt_status_consulta"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:text="Status"
            android:textAppearance="@style/TextAppearance.AppCompat.Display1" />
    </LinearLayout>

</LinearLayout>